<div class="cms-sections">
{% for block in section.blocks %}
	{% case block.type %}

		{% when 'banner-one' %}
		{% include 'cms-banner-one' %}

		{% when 'banner-two' %}
		{% include 'cms-banner-two' %}

		{% when 'banner-three' %}
		{% include 'cms-banner-three' %}

		{% when 'banner-four' %}
		{% include 'cms-banner-four' %}

		{% when 'banner-carousel' %}
			{% assign carousel_size = block.settings.grid | times: 1 %}
			{% assign end_carousel = forloop.index | plus: carousel_size %}
			{% include 'cms-carousel' %}

		{% when 'onecolumn-content' %}
		{% include 'cms-text-one' %}

		{% when 'twocolumn-content' %}
		{% include 'cms-text-two' %}

		{% when 'threecolumn-content' %}
		{% include 'cms-text-three' %}

		{% when 'featured-collection' %}
		{% include 'cms-featured-collection' %}

		{% when 'featured-blog' %}
		{% include 'cms-featured-blog' %}

	{% endcase %}

	{% if end_carousel == forloop.index %}
		{% include 'cms-end-carousel' %}			
	{% endif %}
{% endfor %}
</div>

